{
  "image": "docker.io/kostrykin/mobi-devcontainer-python:2023.1",
  "hostRequirements": {
    "cpus": 2
  },
  "onCreateCommand": "code --disable-extension ms-python.vscode-pylance .",
  "customizations": {
    "codespaces": {
      "openFiles": []
    },
    "vscode": {
      "settings": {
        "files.exclude": {
          "**/.*": true,
          "LICENSE": true,
          "images": true
        }
      }
    }
  }
}
